<table class="ttt-board">
    <tbody>
        {{#each board}}
            <tr id="ttt-row-{{@index}}" class="ttt-row">
            {{#each this}}
                {{#hasSymbol this}}
                    <td id="ttt-field-{{@index}}" class="ttt-field">
                        {{{symbol this}}}
                    </td>
                {{else}}
                    <td id="ttt-field-{{@index}}" class="ttt-active ttt-field">
                    </td>
                {{/hasSymbol}}
            {{/each}}
            </tr>
        {{/each}}
    </tbody>
</table>